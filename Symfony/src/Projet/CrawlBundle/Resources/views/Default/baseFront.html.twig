{% extends 'base.html.twig' %}

{% block css %}
    <link rel="stylesheet" href="{{asset('bundles/main/css/style.css')}}" />
{% endblock %}

{% block body %}
    <div class="page-content">
        <div class='container'>
            {% include 'MainBundle:Front:nav.html.twig' %}
            
                {% for flashMessage in app.session.flashbag.get('success') %}
                    <p>{{ flashMessage  }}</p>
                {% endfor %}
                {% for flashMessage in app.session.flashbag.get('error') %}
                    <p>{{ flashMessage  }}</p>
                {% endfor %}
                    
            {% block content %}{% endblock %}
            {% include 'MainBundle:Front:footer.html.twig' %}
        </div>
    </div>
    {% include 'MainBundle:Front:form-log.html.twig' %}
    {% include 'MainBundle:Front:form-ins.html.twig' %}
{% endblock %}
    
{% block javascripts %}
    <script src="{{ asset('bundles/main/js/jquery.js') }}" type="text/javascript" /></script>
    <script src="{{ asset('bundles/main/js/velocity.js') }}" type="text/javascript" /></script>
    {# <script src="{{ asset('bundles/main/js/velocity-ui.js') }}" type="text/javascript" /></script> #}
    <script src="{{ asset('bundles/main/js/app.js') }}" type="text/javascript" /></script>
{% endblock %}